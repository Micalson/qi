set at_Z     -8.813446044921875

vviewparams ${scale} ${center_X} ${center_Y} \
		${proj_X} ${proj_Y} ${proj_Z} \
		${up_X} ${up_Y} ${up_Z} \
		${at_X} ${at_Y} ${at_Z}


