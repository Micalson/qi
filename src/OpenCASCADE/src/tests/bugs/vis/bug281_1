puts "========"
puts "OCC281"
puts "========"

vinit

set x 205
set y 208
set TypeOfMarker 0

set scale    50.0521
set center_X 0
set center_Y 0
set proj_X   0.57735
set proj_Y   -0.57735
set proj_Z   0.57735
set up_X     -0.408248
set up_Y     0.408248
set up_Z     0.816497
set at_X     0
set at_Y     0
set at_Z     0

vviewparams ${scale} ${center_X} ${center_Y} ${proj_X} ${proj_Y} ${proj_Z} ${up_X} ${up_Y} ${up_Z} ${at_X} ${at_Y} ${at_Z}

OCC281 ${x} ${y} ${TypeOfMarker}
OCC281 ${x} ${y} ${TypeOfMarker}

OCC281 200 200 0
OCC281 200 200 0
puts "Type of marker mast be blue point!"

set only_screen 1
